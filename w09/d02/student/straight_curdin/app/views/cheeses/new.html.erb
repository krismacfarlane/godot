<h2>Enter a new cheese!!!</h2>

<%= form_for(@cheese) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :milk_type, "Type of Milk" %>
  <%= f.text_field :milk_type %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :image_url %>
  <%= f.text_field :image_url %>
  <%= f.submit %>
<% end %>
